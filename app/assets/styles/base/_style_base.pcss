/* Containers
----------------------------------------------------------------------------------------------------*/
.container_16 {
  margin-left: auto;
  margin-right: auto;
  width: auto;

  @media (min-width: 768px) {
    width: 750px;
  }
  @media (min-width: 992px) {
    width: 970px;
  }
  @media (min-width: 1200px) {
    width: 1170px;
  }
}

/* Grid >> Global
----------------------------------------------------------------------------------------------------*/

.grid_1,
.grid_2,
.grid_3,
.grid_4,
.grid_5,
.grid_6,
.grid_7,
.grid_8,
.grid_9,
.grid_10,
.grid_11,
.grid_12,
.grid_13,
.grid_14,
.grid_15,
.grid_16 {
  display: block;
  float: left;
  position: relative;
  margin-left: 15px;
  margin-right: 15px;
}

/* Grid >> Children (Alpha ~ First, Omega ~ Last)
----------------------------------------------------------------------------------------------------*/

.alpha {
  margin-left: 0;
}

.omega {
  margin-right: 0;
}

/* Grid >> 16 Columns
----------------------------------------------------------------------------------------------------*/

.container_16 .grid_1 {
  width: calc(6.25% - 30px);
}

.container_16 .grid_2 {
  width: calc(12.5% - 30px);
}

.container_16 .grid_3 {
  width: calc(18.75% - 30px);
}

.container_16 .grid_4 {
  width: calc(25% - 30px);
}

.container_16 .grid_5 {
  width: calc(31.25% - 30px);
}

.container_16 .grid_6 {
  width: calc(37.5% - 30px);
}

.container_16 .grid_7 {
  width: calc(43.75% - 30px);
}

.container_16 .grid_8 {
  width: calc(50% - 30px);
}

.container_16 .grid_9 {
  width: calc(56.25% - 30px);
}

.container_16 .grid_10 {
  width: calc(62.5% - 30px);
}

.container_16 .grid_11 {
  width: calc(68.75% - 30px);
}

.container_16 .grid_12 {
  width: calc(75% - 30px);
}

.container_16 .grid_13 {
  width: calc(81.25% - 30px);
}

.container_16 .grid_14 {
  width: calc(87.5% - 30px);
}

.container_16 .grid_15 {
  width: calc(93.75% - 30px);
}

.container_16 .grid_16 {
  width: calc(100% - 30px);
}

/* Prefix Extra Space >> 16 Columns
----------------------------------------------------------------------------------------------------*/

.container_16 .prefix_1 {
