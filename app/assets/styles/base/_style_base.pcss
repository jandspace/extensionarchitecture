  font-weight: 700;
}

blockquote {
  margin: 1em 40px;
  quotes: none;
  border-left: 1px solid #cccccc;
  font-style: italic;
  margin-left: 2em;
  padding: 1.5em;
  margin-bottom: 1.5em;
}

/*
