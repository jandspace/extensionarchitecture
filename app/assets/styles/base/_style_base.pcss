#header .quote-a-free-request .button-md {
  display: flex;
  align-self: center;
  justify-content: center;
  font-size: 14px;
  text-transform: uppercase;
  padding: 14px 20px;
  border-radius: 30px;
  line-height: 1.1em;
  text-align: center;
  box-shadow: none;
  font-weight: 700;
  z-index: 1;
  transition: all 0.2s ease;
  background: $color-maroon;

  &:hover {
    color: $color-maroon-darker;
    background-color: $color-sand;
    margin-top: -4px;
    border-bottom: 4px solid $color-maroon;
  }
}

#header .quote-a-free-request .button-md:before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  height: 100%;
  z-index: -1;
  background-color: $color-maroon-dark;
  -webkit-transform: scaleY(0.3);
  transform: scaleY(0.3);
  opacity: 0;
  transition: all 0.3s;
}

@-webkit-keyframes BGGradAni {
  0% {
    background-position: 100% 50%;
  }

  100% {
    background-position: 0 50%;
  }
}

@-moz-keyframes BGGradAni {
  0% {
    background-position: 100% 50%;
  }

  100% {
    background-position: 0 50%;
  }
}

@keyframes BGGradAni {
  0% {
