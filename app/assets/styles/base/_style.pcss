}

/* Custom Duo Table */
.duo-table {
  width: 100%;
  margin: 40px 0px 30px;
}

.duo-table th,
.duo-table td {
  border: 1px solid $color-maroon;
  height: 50px;
  padding: 0 25px;
  box-sizing: border-box;
  text-align: left;
}

.duo-table th {
  color: $color-maroon;
  background-color: #f2f2f2;
  font-size: 18px;
  line-height: 29px;
  letter-spacing: 0.05em;
  font-weight: bold;
}

.duo-table tr td:first-child {
  background: #f2f2f2;
  font-weight: bold;
  color: #333;
}

.duo-table th:first-child,
.duo-table td:first-child {
  width: 25%;
  min-width: 143px;
}

.duo-table td {
  color: #4d4d4d;
  font-size: 18px;
  letter-spacing: 0.05em;
  line-height: 29px;
}

.duo-table tr:nth-child(odd) {
  background: #f2f2f2;
}

.duo-table tr td {
  border-top: none;
  border-bottom: none;
}

.duo-table tr:last-child td {
  border-bottom: 1px solid $color-maroon;
}

