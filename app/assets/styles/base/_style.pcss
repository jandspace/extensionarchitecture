/*----------------------------------------
		Page Navigaton (wp-pagenavi)
---------------------------------------- */
.pagenavi {
  clear: both;
  padding: 0px 0 25px 0px;
  margin-bottom: 2rem;
  position: relative;
  overflow: hidden;
}
.author .pagenavi {
  padding: 0px 0 40px 0;
}

.pagenavi .pages {
  float: right;
  margin: 0px 0px 4px 10px;
  display: none;
}

.pagenavi span,
.pagenavi a {
  display: inline-block;
  position: relative;
  float: left;
  margin: 0px 3px 3px 0;
  padding: 8px 3px;
  border: 1px solid #ccc;
  border-radius: 6px;
  text-align: center;
  min-width: 32px;
