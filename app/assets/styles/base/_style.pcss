.share-buttons {
  background-color: #fff;
  padding: 10px;
  color: #000;
  letter-spacing: 0.05em;
  margin-bottom: 6px;
  border-radius: 8px;

  span {
    display: block;
    font-size: 12px;
    text-decoration: none;
    color: $color-maroon;
    font-weight: 700;
    text-transform: uppercase;
  }
}

.share-buttons a {
  width: 32px;
  height: 32px;
  display: block;
  margin: 8px auto;
  transition: all 1s ease;
  background-repeat: no-repeat;
}

.share-buttons a:hover {
  filter: brightness(0.6);
}

.fb-icon {
  background-image: url(/wp-content/themes/eatheme2020/images/fb-icon.png);
}

.tw-icon {
  background-image: url(/wp-content/themes/eatheme2020/images/tw-icon.png);
}

.return-blog {
  color: #fff;
  padding: 5px;
  line-height: 1.4;
}

.return-blog a {
  display: block;
  color: #fff;
  font-weight: bold;
  text-decoration: none;
  letter-spacing: 0.05em;
  transition: all 1s ease;
  &:hover {
    text-decoration: none;
    /* color: $color-maroon-dark; */
  }
}

.stick {
  position: fixed;
  top: 46vh;
  transform: translateY(-50%);
  z-index: 99;
  transition: opacity 1s ease;
  opacity: 1;

  left: calc((100vw - 930px) - (100vw - 940px) / 2);

  @media (min-width: 1440px) {
    left: calc((100vw - 940px) - (100vw - 940px) / 2);
  }
}

.no-opacity {
  opacity: 0;
  pointer-events: none;
}

.single .post-holder.new-post {
  margin-bottom: 0;
  padding-bottom: 0;
}

.content.contact-wrapper {
  background-color: $color-maroon;
  padding: 50px;
  display: flex;
  justify-content: space-between;
}

.landing-margins .content.contact-wrapper {
  padding: 30px 50px;
}

.contact-block,
.contact-block a {
  color: #fff;
}

.contact-block {
  font-size: 25px;
  line-height: 29px;
  letter-spacing: 0.05em;
}

.contact-block:first-child {
  border-right: 1px solid #fff;
  flex: 0 1 544px;
  padding-right: 24px;
  box-sizing: border-box;
}

.contact-block span {
  display: block;
}

.contact-title {
  margin-top: 5px;
  margin-bottom: 27px;

  font-weight: bold;
}

.landing-margins .contact-title {
  margin-bottom: 4px;
  text-decoration: underline;
}

.contact-text {
  margin-bottom: 5px;
}

.contact-block:last-child {
  padding-left: 25px;
  box-sizing: border-box;
  text-align: center;
}

.blog-tel-link {
  border: 3px solid #fff;
  padding: 16px 23px;
  display: block;
  font-size: 23px;
  font-weight: bold;
  text-decoration: none !important;
  margin-top: 21px;
}

.subscribe-container {
  margin: 30px 0 0 0;
  padding: 0;
  .subscribe-item {
    margin: 0 auto;
    background-color: #f4f4f4;
    border-top: 6px solid $color-maroon;
    padding: 6% 0;
    text-align: center;
    h2 {
      margin: 0 auto;
      color: $color-maroon-dark;
    }
    p {
      color: #303030;
      font-size: 18px;
    }
    .NEWLET {
      .newsletter-form {
        input {
          width: 100%;
          max-width: 360px;
          margin: 6px auto;
          display: block;
          padding: 12px 18px;
          border-radius: 8px;

          &[type="email"] {
            color: #202020;
            border: 1px solid rgba(104, 91, 199, 0.6);
            background-color: rgba(255, 255, 255, 0.6);
          }
        }
      }
    }
  }
}

/*----------------------------------------
	Carousel Slider Plugin custom
---------------------------------------- */
.owl-carousel {
  &.carousel-slider {
    .owl-stage {
      a {
        text-decoration: none;
        img {
          margin: 6px 0;
        }
        .carousel-slider__caption {
          h4 {
            text-transform: none;
            font-size: 14px;
          }
        }
      }
    }
  }
}

/*----------------------------------------
				 Comments, including old code
---------------------------------------- */
.content.comments-wrapper {
  padding: 0 95px;
}

h3#comments {
  font-size: 31px;
  line-height: 34px;
  color: #606060;
  font-weight: 300;

  text-transform: uppercase;
  margin: 0 0 15px 0;
}

ol.commentlist {
  list-style-type: none;
  padding: 0 0 44px 0;
  margin: 0;
}

li.comment {
  list-style-type: none;
  overflow: hidden;
  padding: 0;
  margin: 0 !important;
  background: none;
  text-transform: none;
}

li.comment .comment-body {
  padding: 25px 20px 30px 0;
  position: relative;
  zoom: 1;
  background: #fff;
  overflow: hidden;
}
li.comment .comment-body p {
  font-size: 18px;
  line-height: 21.6px;
  letter-spacing: 0.01em;

  margin: 22px 0;
}

/* Avatar */
li.comment .avatar {
  display: block;
  vertical-align: top;
  overflow: hidden;
  border-radius: 50%;
}

.talignright {
  text-align: right;
}

/* Comment meta */
.comment-meta {
  font-size: 12px;
  line-height: 29px;
  color: #606060;
  display: inline-block;
  float: right;
  font-weight: bold;
}
.comment-meta a {
  color: #606060;
}

.commentlist ul.children {
  margin: 0;
  padding: 0;
  overflow: hidden;
}

.commentlist ul.children li .comment-body {
  padding: 0 20px;
  margin: 25px 0;
  border-left: 1px solid $color-maroon;
}

/* Reply */
.reply {
  display: inline-block;
}
.reply a {
  font-size: 18px;
  line-height: 21.6px;
  color: #808080;
  padding-right: 20px;
  letter-spacing: 0.01em;
  text-decoration: none;
  text-transform: uppercase;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.reply a:hover {
  color: #606060;
  text-decoration: none;
}

.reply-button {
  margin-bottom: -8px;
}

li.comment .avatar {
  width: 35px;
  height: 35px;
  display: inline-block;
  vertical-align: middle;
}

.author-comment,
.author-comment a {
  display: inline-block;
  vertical-align: middle;
  color: $color-maroon;
  text-transform: capitalize;

  font-weight: 500;
  margin-left: 10px;
  font-size: 18px;
  margin-top: 0;
  letter-spacing: 0.05em;
  line-height: 29px;
  text-decoration: none;
}

.comment-author {
  margin: 0 20px 4px 0;
}

.commentlist ul.children li {
  list-style: none;
}

.comment.depth-1 {
  border-bottom: 1px solid #808080;
  padding-top: 35px;
  padding-bottom: 45px;
}

.blog-background .posts-list-box {
  clear: both;
  background-color: #fff;
  padding-top: 1px;
}

.posts-list-heading h3 {
  text-transform: uppercase;
  font-size: 30px;
  letter-spacing: 0.01em;
  line-height: 0px;
}

.blog-background .posts-list-box {
