    top: unset;
    width: 100%;
    margin: 0;
    transform: translateY(0);
    padding: 0;
  }

  .share-buttons {
    bottom: 0px;
    width: 100%;
    height: 37px;
    box-sizing: border-box;
    padding: 0;
