.zoom-icon {
  display: block;
  position: absolute;
  left: 6px;
  top: 6px;
  right: 6px;
  bottom: 6px;
  cursor: pointer;
  background: url(/wp-content/themes/eatheme2020/images/bg-zoom.png) 0 0 repeat;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  opacity: 0;
  z-index: 2;
}
.no-rgba .zoom-icon {
  background: url(/wp-content/themes/eatheme2020/images/bg-zoom.png) 0 0 repeat;
  visibility: hidden;
}
.zoom-icon:before {
  content: "";
  display: block;
  position: absolute;
  left: 50%;
  top: 50%;
  width: 101px;
  height: 101px;
  margin: -50px 0 0 -50px;
  background: url(/wp-content/themes/eatheme2020/images/magnify-alt.png) 50% 50% no-repeat;
}
.image-wrap:hover .zoom-icon {
  opacity: 1;
}
.no-rgba .image-wrap:hover .zoom-icon {
  visibility: visible;
}

