  text-align: center;
  background-color: $color-maroon;
  overflow: hidden;
  transition: all 1.5s ease-out;

  &:before,
  &:after {
    content: "";
    display: block;
    position: absolute;
    border-radius: 50%;
    box-shadow: 0 0 36px rgba(46, 0, 139, 0.32);
    transition: all 1s ease-out;
