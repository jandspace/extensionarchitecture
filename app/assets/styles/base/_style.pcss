/*---------------------------------------- 
					Post
---------------------------------------- */
.header-title {
  margin-bottom: 20px;
}
.header-title h1 {
  font-size: 32px;
  line-height: 36px;
  font-weight: 700;
  margin: 0 0 20px 0;
  background: url(/wp-content/themes/eatheme2020/images/title-shadow.png) center bottom no-repeat;
  text-align: center;
  padding: 14px 0 46px 0;
}

.post-holder {
  margin: 0 0px 46px 0;
  position: relative;
  overflow: hidden;
  padding: 0 0 46px 0;
  border-bottom: 1px dotted #dddddd;
}
@media (min-width: 1200px) {
  .post-holder {
    margin-right: 5%;
  }
  .post-holder:hover .featured-thumbnail a img {
    transform: scale(1.25);
    transition: transform 5s linear;
  }
}
.single .post-holder {
  padding: 0 0 25px 0;
  margin: 0 0px 37px 0;
}

.post-holder.no-border {
  border: none;
  padding: 0;
}

/* Post header */
.entry-header {
  margin-bottom: 10px;
}
@media (min-width: 1200px) {
  .entry-header {
    padding: 0 15px;
  }
}
.single .entry-header {
  padding-top: 2px;
  margin-bottom: 25px;
}

.post-holder .entry-title,
.single-post .entry-title {
  font-size: 20px;
  color: #202020;
  margin: 0 0 8px 0;
  padding: 0;
  text-transform: none;
  font-weight: 700;
  letter-spacing: normal;
}
@media (min-width: 992px) {
  .post-holder .entry-title,
  .single-post .entry-title {
    font-size: 20px;
  }
}
@media (min-width: 1200px) {
  .post-holder .entry-title,
  .single-post .entry-title {
    font-size: 28px;
  }
}
.post-holder .entry-title a {
  display: inline-block;
  text-decoration: none;
  color: #202020;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.post-holder .entry-title a:hover {
  color: #202020;
}
.single-post .entry-title {
  font-size: 24px;
  margin-bottom: 10px;
}

/* Meta information for post */
.post-meta {
  font-size: 13px;
  line-height: 18px;
  color: #aaaaaa;
}
.post-meta a {
  text-decoration: none;
  color: #606060;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.post-meta a:hover {
  color: #202020;
}

/* Featured thumbnail */
.featured-thumbnail {
  position: relative;
  float: left;
  margin: 5px 20px 10px 0;
  /* border:1px solid #ececec; */
  padding: 6px;
}
.two_third .featured-thumbnail {
  margin: 0;
}
.services .featured-thumbnail {
  padding: 0;
  border: none;
}
.services .featured-thumbnail .stroke {
  display: none;
}

.featured-thumbnail a {
  display: block;
  text-decoration: none;
}
.featured-thumbnail img {
  position: relative;
  z-index: 1;
}
.portfolio-view.featured-thumbnail .stroke {
  display: none;
}

.stroke {
  position: absolute;
  border: 5px solid #fff;
  top: 6px;
  left: 6px;
  bottom: 6px;
  right: 6px;
  z-index: 2;
  opacity: 0;
  filter: alpha(opacity=0);
  display: block;
}

/* Large thumbnail */
.featured-thumbnail.large {
  overflow: hidden;
  width: 100%;
  padding: 0;
  margin: 0px 0px 27px 0;
  border-radius: 8px;
}
@media (min-width: 1200px) {
  .featured-thumbnail.large {
    border-radius: 15px;
  }
}
.featured-thumbnail.large a {
  display: block;
}
.featured-thumbnail.large a img {
  width: 100%;
  height: auto;
  max-width: none;
}

/* Post content */
.post-content {
  padding: 0;
}
@media (min-width: 1200px) {
  .post-content {
    padding: 0 15px;
  }
}
.post-content .excerpt {
  font-size: 14px;
  margin-bottom: 12px;
}

/* Post footer */
.post-holder .post-footer {
  clear: both;
  padding: 0 0 0 0px;
  overflow: hidden;
}
.post-holder .post-footer a {
  float: left;
  font-weight: 700;
  font-size: 15px;
}
.post-holder .post-footer a.link {
  padding: 8px 30px 8px 8px;
  color: $color-maroon;
  border: 1px solid $color-maroon;
  background-position: right 8px top 11px;
}
.post-holder .post-footer a.link:hover {
  background-color: rgba(104, 91, 187, 0.2);
  text-decoration: none;
}

.comments-link {
  display: none;
  /*	display:inline-block; */
  color: #606060;
  font-size: 14px;
  line-height: 20px;
  margin-left: 21px;
  text-transform: none;
  text-decoration: none;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
a.comments-link:hover {
  color: #202020;
  text-decoration: none;
}

.nocomments {
  padding-bottom: 44px;
  display: block;
}

p.gravatar {
  float: left;
  margin-right: 20px;
}

/* Post edit link */
.post-edit-link {
}
.post-edit-link:hover {
}

/*---------------------------------------- 
				Related Posts
---------------------------------------- */
