/*----------------------------------------
				Navigations
---------------------------------------- */
/* Primary navigation */
nav.primary {
  float: right;
  padding: 0;
  margin: 0 -12px -12px 0;
  background: none;
  z-index: 99;
  display: inline-block;
}

#topnav {
  margin: 0;
}

/* Essential styles for dropdown menu */
.sf-menu {
  padding: 0px 0px 0px 0px;
  position: relative;
  background: none;
  text-align: left;
}
.sf-menu li {
  position: relative;
  list-style: none;
}
.sf-menu > li {
  display: block;
  float: left;
  text-align: center;
  position: relative;
  margin: 0 2px 0 1px;
  padding: 0;
  background: transparent;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.sf-menu > li:first-child {
  /*	background:#fff; */
}

.sf-menu > li:hover,
.sf-menu > li.sfHover,
.sf-menu > li.current-menu-item {
  /*	background-color:#202020; */
  background-position: 0 -300px;
  /* border-bottom: 4px solid #202020; */
}
.sf-menu > li:hover {
  /* border-bottom: 4px solid #808080;
	margin-bottom: -4px!important; */
}

.sf-menu li a {
  display: block;
  text-decoration: none;
  position: relative;
  z-index: 3;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
.sf-menu > li > a {
  font-size: 14px;
  line-height: 1.28;
  background: none;
  /* border-bottom: 4px solid transparent; */
  text-transform: none;
  text-decoration: none;
  padding: 20px 28px;
  color: #202020;
  font-weight: 700;
  text-transform: uppercase;
}

/* @media screen and (min-width: 992px) {
	.sf-menu > li > a {
		padding:30px 16px;
	}
}
@media screen and (min-width: 1200px) {
	.sf-menu > li > a {
		padding:30px 28px;
	}
} */

.sf-menu > li:hover > a,
.sf-menu > li.sfHover > a {
  /* background-color: $color-sand; */
  /* color: #ffffff; */
  text-decoration: none;
}
.sf-menu > li.current-menu-item > a {
}

.sf-menu > ul > li:last-child > ul {
  right: 0 !important;
  left: inherit !important;
}
.sf-menu ul {
  position: absolute;
  width: 240px; /* left offset of submenus need to match (see below) */
  padding: 15px 17px 10px 17px;
  z-index: 200;
  display: none;
  margin: 0;
  left: 0;
  top: 100%;
  z-index: 99;
}
.sf-menu > li > ul > li a {
  font-weight: 700;
}
.sf-menu > li > ul > li > ul > li a {
  font-weight: 300;
}

@media (min-width: 992px) {
  .sf-menu > li > ul {
    box-shadow: 0 8px 14px rgba(0, 0, 0, 0.5);
  }
}

ul.sf-menu li:hover li ul,
ul.sf-menu li.sfHover li ul {
  width: 150px;
  padding: 15px 17px 10px 17px;
  margin: 0;
  /* background:#202020; */
}
ul.sf-menu li li:hover ul,
ul.sf-menu li li.sfHover ul {
  left: 185px; /* match ul width */
  top: 0;
  z-index: 99;
}
.sf-menu ul li,
.sf-menu ul li li {
  margin: 0 0 8px 0;
  padding: 7px 0 0 0;
  text-align: center;
  background: none;
  display: block;
  /* border-top:1px solid #5e5b53; */
}
.sf-menu ul li:first-child,
.sf-menu ul li li:first-child {
  border-top: none;
  padding-top: 0px;
}

.sf-menu ul li li {
  border-top-color: #7d766d;
}

.sf-menu li ul li a {
  text-align: left;
  color: #ffffff;
  font-size: 14px;
  line-height: 16px;
  padding: 0;
  background: none;
  display: block;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
/* .sf-menu li ul li ul li a {
	opacity: 0.8;
} */

.sf-menu li li.current-menu-item > a,
.sf-menu li li.sfHover > a,
.sf-menu li li:hover > a {
  color: #ffffff;
  text-decoration: none;
}

body:not(.logged-in) .sf-menu li.hidden-menu {
  display: none !important;
}
body .sf-menu li.hidden-menu a {
  color: red !important;
  align-items: flex-start !important;

  &:before {
    content: "✗";
    display: inline-block;
    margin: -4px 6px 0 0;
  }
}

