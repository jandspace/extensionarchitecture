/*----------------------------------------
				Forms
---------------------------------------- */

/* Form defaults */
input,
select,
textarea {
  font-size: 14px;
  line-height: 20px;
  color: #202020;
  vertical-align: middle;
  font-weight: normal;
  border: 1px solid #dddddd;
  outline: none;
}
input[type="text"],
input[type="email"],
input[type="tel"],
textarea {
  background: none;
  border: 1px solid #dddddd;
  padding: 6px 10px 6px 10px;
  border-radius: 0px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  resize: none;
}
input[type="submit"],
input[type="reset"] {
  position: relative;
  display: inline-block;
  padding: 6px 40px;
  height: auto;
  font-size: 13px;
  line-height: 20px;
  color: #fff;
  text-decoration: none;
  text-transform: uppercase;
  background: $color-maroon-dark;
  /*	background:url(/wp-content/themes/eatheme2020/images/link-marker.png) 100% center no-repeat; */
  border: none;
  cursor: pointer;
  margin-right: 10px;
  -webkit-transition: all 0.5s ease;
  -moz-transition: all 0.5s ease;
  -o-transition: all 0.5s ease;
  transition: all 0.5s ease;
}
input[type="submit"]:hover,
input[type="reset"]:hover,
input[type="submit"].submit-hover,
input[type="reset"].submit-hover {
  color: #606060;
}
input[type="submit"]:hover,
input[type="reset"]:hover {
  color: #fff;
  background: #808080;
}

/* Comment form */

#commentform a {
  display: inline-block;
  background: none;
  border-radius: 0px;
  color: #202020;
  text-decoration: underline;
  text-transform: none;
  font-size: 14px;
  padding: 0;
}
#commentform a:hover {
  text-decoration: none;
}

#commentform p.field {
  margin-bottom: 13px;
}
#commentform p.area {
  margin-bottom: 15px;
}
#commentform small {
  font-size: 12px;
  line-height: 18px;
  color: #828082;
}
#commentform label {
  display: block;
  padding-bottom: 5px;
  color: #202020;
}
#commentform input[type="text"] {
  width: 280px;
  height: 33px;
}
#commentform textarea {
  width: 460px;
  height: 182px;
  overflow: auto;
